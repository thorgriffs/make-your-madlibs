<div data-aos="zoom-in-down" class="jumbotron">
  <img class="img-fluid rounded mx-auto d-block max-width: 100%;" src="./images/libarch.png" alt="">
</div>

{{#each stories}}
<div class="row mb-5">
  <div class="col-2"></div>
  <div class="col-10">
    <div data-aos="flip-up" class="card mt-4" id="storiesCard" style="width: 80%;">
      <div class="card-body">
        <h4 class="card-title text-center m-2 pb-3" id="storiesTitle" style="font-weight: bold;letter-spacing: 5px">
          {{this.title}}</h5>
          <p class="pl-4 pr-4 pb-2">{{this.storyBody}}</p>
      </div>
    </div>
  </div>
</div>
{{/each}}
